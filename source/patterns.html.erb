---
title: Refills
---
<div class="refills-patterns">
  <a class="js-menu-trigger-refills refills-menu-anchor fixedsticky">
    <img src="/images/menu.png" alt="Menu icon">
  </a>
  <div class="js-menu-screen-refills refills-menu-screen"></div>

  <%= partial "refills-hero" %>
  <%= partial "refills-menu" %>

  <div id="example">

    <div id="navigation" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Navigation</h6></div>
        <%= partial "refills/navigation" %>
      </div>
      <div class="refill-snippet"><%= code_for('navigation') %></div>
    </div>

    <div id="centered-navigation" class="refill">
      <div class="refills-wrapper">
        <%= partial "refills/centered_navigation" %>
      </div>    
    </div>

    <div id="hero" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Hero Unit</h6></div>
        <%= partial "refills/hero" %>
      </div>
    </div>

    <div id="intro_text" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Intro Text</h6></div>
        <%= partial "refills/intro_text" %>
      </div>
    </div>

    <div id="icon-bullet-points" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Icon Bullet Points</h6></div>
        <%= partial "refills/icon_bullet_points" %>
      </div>
    </div>

    <div id="device" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Device</h6></div>
        <%= partial "refills/device" %>
      </div>
    </div>

    <div id="side-image" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Side Image</h6></div>
        <%= partial "refills/side_image" %>
      </div>
    </div>

    <div id="search-tools" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Search Tools</h6></div>
        <%= partial "refills/search_tools" %>
      </div>
    </div>

    <div id="cards" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Cards</h6></div>
        <div class="refill-centering"><%= partial "refills/cards" %></div>
      </div>
    </div>

    <div id="browser" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Browser</h6></div>
        <%= partial "refills/browser" %>
      </div>
    </div>

    <div id="comment" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Comments</h6></div>
        <div class="refill-centering">
          <div class="refill-smaller"><%= partial "refills/comment" %></div>
        </div>
      </div>
    </div>

    <div id="accordion-tabs" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Accordion / Tabs</h6></div>
        <%= partial "refills/accordion_tabs" %>
      </div>
      <div class="refills-wrapper">
        <%= partial "refills/accordion_tabs_minimal" %>
      </div>
    </div>

    <div id="tables" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Tables</h6></div>
        <%= partial "refills/tables" %>
      </div>
      <div class="refills-wrapper">
        <%= partial "refills/tables_minimal" %>
      </div>
    </div>

    <div id="vertical-tabs" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Vertical Tabs</h6></div>
        <%= partial "refills/vertical_tabs" %>
      </div>
    </div>

    <div id="grid-items" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Grid Items</h6></div>
        <div class="refill-centering"><%= partial "refills/grid_items" %></div>
      </div>
      <div class="refills-wrapper">
        <div class="refill-centering"><%= partial "refills/grid_items_lines" %></div>
      </div>
    </div>

    <div id="footer" class="refill">
      <div class="refills-wrapper">
        <div class="line-behind-text"><h6>Footers</h6></div>
        <div class="refill-centering"><%= partial "refills/footer" %></div>
      </div>

      <div class="refills-wrapper">
        <div class="refill-centering"><%= partial "refills/footer_2" %></div>
      </div>
      
    </div>
  </div>
</div>

<%= partial "refills-page-scripts" %>
